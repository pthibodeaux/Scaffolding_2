{
	"Logging": {
		"IncludeScopes": false,
		"Debug": {
			"LogLevel": {
				"Default": "Debug",
				"System": "Information",
				"Microsoft": "Information"
			}
		},
		"Console": {
			"LogLevel": {
				"Default": "Debug",
				"System": "Information",
				"Microsoft": "Information"
			}
		},
		"NLog": {
			"LogLevel": {
				"Default": "Debug",
				"Service*": "Error",
				"*Controller": "Trace",
				"ErrorHandlingMiddleware": "Info"
			}
		}
	}
}
